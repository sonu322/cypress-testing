  "scripts": {
    "start": "parcel serve --no-source-maps public/issueTreeModuleEntry.html public/tracebilityReportModuleEntry.html",
    "build-data": "npx tsc data/index.ts --outDir dist/data",
    "generate-data": "node dist/data/index.js",
    "prettify": "npx prettier --write .",
    "prebuild-dev": "/bin/bash prebuild_dev.sh",
    "prebuild": "/bin/bash prebuild.sh",
    "build": "parcel build --no-source-maps --no-minify public/*.html",
    "build-server": "rm -rf dist && parcel build --no-source-maps --no-minify src/IssueLinksHierarchyServer.tsx src/TracebilityReportServer.tsx && cp dist/IssueLinksHierarchyServer.js dist/TracebilityReportServer.js ../lxp-server/src/main/resources/includes/js/",
    "build-dev": "parcel build --no-source-maps --no-minify public/*.html",
    "upload-dev": "aws --profile otpl s3 sync ./dist s3://lxp-cloud-dev --delete",
    "upload": "aws --profile otpl s3 sync ./dist s3://lxp-cloud",
    "serve": "node serve.js",
    "flow": "flow",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix"
  },
  "staticFiles": {
    "staticPath": [
      "assets"
    ],
    "watcherGlob": "**"
  },
  "browserslist": "> 0.25%, not dead"
}
