    "@atlaskit/tree": "^6.0.0",
    "@ungap/url-search-params": "^0.1.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.6",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
