{
  "name": "Links Explorer",
  "description": "This is simple links tree view plugin",
  "key": "com.otpl.jira.plugins.lxp",
  "baseUrl": "https://lxp.optimizoryapps.com",
  "vendor": {
    "name": "Optimizory Technologies Private Limited",
    "url": "https://optimizory.com"
  },
  "links": {
    "self": "https://lxp.optimizoryapps.com/atlassian-connect.json",
    "documentation": "https://optimizory.atlassian.net/l/cp/RQM6FPJB"
  },
  "enableLicensing": true,
  "translations": {
    "paths": {
      "en-US": "/i18n/0.4/en_US.json",
      "de-DE": "/i18n/0.4/de_DE.json",
      "es-ES": "/i18n/0.4/es_ES.json",
      "fr-FR": "/i18n/0.4/fr_FR.json"
    }
  },
  "authentication": {
    "type": "none"
  },
  "scopes": ["read", "write"],
  "apiVersion": 1,
  "modules": {
    "jiraIssueTabPanels": [
      {
        "url": "/issueTreeModuleEntry.html",
        "weight": 100,
        "name": {
          "value": "Links Explorer",
          "i18n": "otpl.lxp.issue.tab.name"
        },
        "key": "lxp-issue-tab"
      }
    ],
    "generalPages": [
      {
        "url": "/tracebilityReportModuleEntry.html",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "Links Explorer",
          "i18n": "otpl.lxp.general.page.name"
        },
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ],
        "key": "lxp-home-page"
      }
    ],
    "configurePage": {
      "key": "my-gadget-configuration",
      "name": {
        "value": "My Gadget Configuration"
      },
      "url": "/ConfigureGadget.html",
      "conditions": [
        {
          "condition": "user_is_logged_in"
        }
      ]
    },
    "jiraReports": [
      {
        "url": "/tracebilityReportModuleEntry.html",
        "description": {
          "value": "This is a traceability report",
          "i18n": "otpl.lxp.traceability-report.description"
        },
        "reportCategory": "agile",
        "thumbnailUrl": "http://example.com/images/report-example-thumbnail.png",
        "name": {
          "value": "Links Explorer Report",
          "i18n": "otpl.lxp.traceability-report.name"
        },
        "key": "lxp-traceability-reports"
      }
    ],
    "jiraDashboardItems": [
      {
        "description": {
          "value": "Dashboard item description",

          "authentication": "none"
        },

        "url": "/lxpViewGadget.html",
        "configurable": true,
        "configure": {
          "url": "/configureGadget.html",
          "height": 500
        },
        "thumbnailUrl": "http://example.com/images/report-example-thumbnail.png",
        "refreshable": false,
        "name": {
          "value": "Dashboard item title"
        },
        "key": "dashboard-item-key"
      }
    ]
  }
}
